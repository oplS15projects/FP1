(define (runif) (- (* 2 (random)) 1))
> (define (rnormish) (+ (runif) (runif) (runif) (runif)))
> (define xs0 (build-list 1000 (位 _ (rnormish))))
> (define ys0 (build-list 1000 (位 _ (rnormish))))
> (define zs0 (build-list 1000 (位 _ (rnormish))))
> (define mags (map (位 (x y z) (sqrt (+ (sqr x) (sqr y) (sqr z))))
                    xs0 ys0 zs0))
> (define xs (map / xs0 mags))
> (define ys (map / ys0 mags))
> (define zs (map / zs0 mags))
> (plot3d (points3d (map vector xs ys zs) #:sym 'dot)
          #:altitude 25)
